import java.util.Scanner;
class A 
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter a number:");
        int n=s.nextInt();
        int count=0;
        int num=n;
        while(num>0)
        {
            count++;
            num/=10;
        }
        System.out.println("the length of number"+" "+n+" "+"is"+" "+count);
    }
}
