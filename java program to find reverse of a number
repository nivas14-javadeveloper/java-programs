import java.util.Scanner;
class A 
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter a number:");
        int n=s.nextInt();
        int res=0;
        int num=n;
        while(num>0)
        {
            int rem=num%10;
            res=rem+(res*10);
            num/=10;
        }
        System.out.println(res);
    }
}
