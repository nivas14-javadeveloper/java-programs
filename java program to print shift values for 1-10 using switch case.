import java.util.Scanner;
class A 
{
    public static void main(String[]args)
    {
        System.out.println("Shift values in keyboard 1-10");
        Scanner s=new Scanner(System.in);
        System.out.println("Enter values between 1-10:");
        int n=s.nextInt();
        switch(n)
        {
            case 1:System.out.println(n+" "+"shift value is"+" !");
            break;
            case 2:System.out.println(n+" "+"shift value is"+" @");
            break;
            case 3:System.out.println(n+" "+"shift value is"+" #");
            break;
            case 4:System.out.println(n+" "+"shift value is"+" $");
            break;
            case 5:System.out.println(n+" "+"shift value is"+" %");
            break;
            case 6:System.out.println(n+" "+"shift value is"+" ^");
            break;
            case 7:System.out.println(n+" "+"shift value is"+" &");
            break;
            case 8:System.out.println(n+" "+"shift value is"+"*");
            break;
            case 9:System.out.println(n+" "+"shift value is"+" (");
            break;
            case 10:System.out.println(n+" "+"shift value is"+" )");
            break;
            default:
            System.out.println("enter number between 1 - 10 only!");
        }
    }
}
