import java.util.Scanner;
class A 
{
    public static void main(String[]args)
    {
        Scanner s=new Scanner(System.in);
        System.out.println("enter a number:");
        int n=s.nextInt();
        System.out.println("the length of a number"+" "+n+" "+"is"+" "+len(n));
    }
    static int len(int n)
    {
        if(n==0)
        {
            return 0;
        }
        return 1+len(n/10);
    }
}
